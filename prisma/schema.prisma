generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id   Int    @id @default(autoincrement())
  name String 
}

model Product {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  fullDescription String?
  price       Float
  image       String?   
  createdAt   DateTime @default(now())
}